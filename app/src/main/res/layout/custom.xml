<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" ><![CDATA[

    />







    ]]>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Course id"
        android:ems="10"
        android:id="@+id/course_id"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="67dp"
        android:layout_marginTop="46dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Date"
        android:ems="10"
        android:layout_below="@+id/course_id"
        android:layout_alignStart="@+id/course_id"
        android:layout_marginTop="41dp"
        android:id="@+id/date_id" />

    <Button
        android:id="@+id/dialogButtonOK"
        android:layout_width="100px"
        android:layout_height="wrap_content"
        android:text=" Ok "
        android:layout_marginRight="5dp"
        android:layout_marginBottom="119dp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/start_hour_id"
        android:layout_marginEnd="23dp" />

    <Button
        android:text="Cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="74dp"
        android:id="@+id/dialogButtonCancel"
        android:layout_alignBaseline="@+id/dialogButtonOK"
        android:layout_alignBottom="@+id/dialogButtonOK"
        android:layout_toStartOf="@+id/dialogButtonOK" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Start Hour"
        android:ems="10"
        android:id="@+id/start_hour_id"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Finish Hour"
        android:ems="10"
        android:id="@+id/finish_hour_id"
        android:layout_above="@+id/dialogButtonOK"
        android:layout_alignStart="@+id/start_hour_id"
        android:layout_marginBottom="11dp" />

</RelativeLayout>